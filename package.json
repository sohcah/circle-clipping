{
  "name": "circle-clipping",
  "packageManager": "yarn@3.6.1",
  "version": "0.0.2",
  "license": "MIT",
  "repository": "https://github.com/sohcah/circle-clipping",
  "workspaces": [
    ".",
    "example"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --minify --format cjs,esm --out-dir dist",
    "release": "yarn build && npm publish --access public",
    "example": "yarn workspace example dev",
    "fuzz": "tsx src/fuzz.ts"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "seed-random": "^2.2.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@turf/area": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "cheap-ruler": "^3.0.2",
    "geokdbush-tk": "^2.0.3"
  }
}
